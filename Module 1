#include<stdio.h>

struct student
{
    char name[50];
    int roll;
    float marks;
};
int main()
{
    struct student s;
    printf("Enter information of student:\n\n");
    printf("Enter the USN\n");
    scanf("%d",&s.roll);
    printf("Enter name: ");
    scanf("%s",s.name);
    printf("Enter marks: \n");
    scanf("%f",&s.marks);
    printf("Displaying Information:\n");
    printf("Roll number: %d\nName: %s\nMarks: %.1f",s.roll,s.name,s.marks);
    return 0;
}
